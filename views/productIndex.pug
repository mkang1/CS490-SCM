extends layout.pug
block title
	title= 'Product List'
block content
	h2= 'Product List'
	table#desc4
		th#desc3= "Serial"
		th#desc3= "Model"
		th#desc3= "Current Location"
		for product in productList
			tr
				td= product.Serial
				td= product.Model
				td= product.CurLocation
	br
	form(action="/inventory/serial" method="get")
		| Serial search: 
		input(type="text" name="serial")
		input(type="submit" value="Submit")
	a(href="view1") View 1